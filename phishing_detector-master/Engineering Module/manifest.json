{
  "manifest_version": 3,
  "name": "Fraud Fence",
  "version": "0.1",
  "permissions": ["tabs"],
  "icons": {
    "16": "fraudfence.png",
    "48": "fraudfence.png",
    "128": "fraudfence.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["jquery-3.1.1.min.js", "content.js"]
    }
  ]
}
